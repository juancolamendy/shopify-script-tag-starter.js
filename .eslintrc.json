{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
        }
    },
    "env": {
        "es6":     true,
        "browser": true,
        "node":    true,
        "mocha":   true
    },   
    "extends": [
        "eslint:recommended"
    ],    
    "rules": {
        "no-console": "off",
        "no-useless-escape": "off",
        "no-undef": "off"
    }
}
